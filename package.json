{
  "name": "auto-iconfont",
  "version": "1.3.1",
  "description": "自动获取 iconfont 相关资源并下载到本地",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "auto-icon": "bin/iconBuild.js"
  },
  "scripts": {
    "dev": "ts-node src/dev.ts",
    "build": "tsc",
    "test": "node bin/iconBuild.js"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "chalk": "^4.1.2",
    "chrome-paths": "^1.0.1",
    "commander": "^9.4.1",
    "css2json": "^1.1.1",
    "download": "^8.0.0",
    "edge-paths": "^2.2.1",
    "fs-extra": "^10.1.0",
    "prettier": "^2.7.1",
    "puppeteer-core": "^18.0.5",
    "qs": "^6.11.0"
  },
  "devDependencies": {
    "@types/download": "^8.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.25",
    "@types/prettier": "^2.7.1",
    "@types/qs": "^6.9.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/space-77/auto-iconfont"
  },
  "bugs": {
    "url": "https://github.com/space-77/auto-iconfont/issues"
  },
  "homepage": "https://github.com/space-77/auto-iconfont#README",
  "author": "space-77",
  "license": "MIT",
  "keywords": [
    "iconfont",
    "iconify",
    "auto"
  ],
  "autoIconfont": {
    "url": "//at.alicdn.com/t/c/font_1849082_3x6or1u6tzg.js",
    "language": "ts",
    "iconify": {
      "enable": true,
      "prefix": "icons"
    },
    "fontFamilyClass": {
      "className": "[class^='icon-']",
      "values": {
        "color": "red",
        "font-size": null
      }
    }
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "printWidth": 120,
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "none",
    "stylelintIntegration": true,
    "htmlWhitespaceSensitivity": "ignore"
  }
}
