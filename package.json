    "build": "tsc",
    "test": "node bin/iconBuild.js"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "chalk": "^5.0.1",
    "chrome-paths": "^1.0.1",
    "commander": "^9.4.1",
    "edge-paths": "^2.2.1",
    "fs-extra": "^10.1.0",
    "prettier": "^2.7.1",
    "puppeteer-core": "^18.0.5",
    "qs": "^6.11.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.25",
    "@types/prettier": "^2.7.1",
    "@types/qs": "^6.9.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/space-77/auto-iconfont"
  },
  "bugs": {
    "url": "https://github.com/space-77/auto-iconfont/issues"
  },
  "homepage": "https://github.com/space-77/auto-iconfont",
  "author": "space-77",
  "license": "MIT",
  "keywords": [
    "iconfont"
  ],
  "autoIconfont": {
    "projectId": "1731015",
    "userInfoPath": "./userInfo.json"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "printWidth": 120,
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "none",
    "stylelintIntegration": true,
    "htmlWhitespaceSensitivity": "ignore"
